        "status":true,
        "title":"Crema catalana",
        "created":"2021-03-18T21:27:49+00:00",
        "changed":"2021-03-18T21:27:49+00:00",
        "promote":true,
        "sticky":false,
        "default_langcode":true,
        "revision_translation_affected":null,
        "moderation_state":"published",
        "path":{
          "alias":"\/recipes\/crema-catalana",
          "pid":81,
          "langcode":"en"
        },
        "content_translation_source":"und",
        "content_translation_outdated":false,
        "field_cooking_time":20,
        "field_difficulty":"medium",
        "field_ingredients":[
          "1l milk",
          " 200g sugar",
          " 6 egg yolks",
          " 30g cornstarch",
          " 1 cinnamon stick",
          " 1 piece lemon peel"
        ],
        "field_number_of_servings":8,
        "field_preparation_time":10,
        "field_recipe_instruction":{
          "value":"\u003Col\u003E\n  \u003Cli\u003EMix the egg yolks, sugar, and cornstarch with 1\/3 of the milk.\u003C\/li\u003E\n  \u003Cli\u003EIn a saucepan, boil 2\/3 of the milk with the lemon peel and cinnamon.\u003C\/li\u003E\n  \u003Cli\u003EAfter 3 minutes, remove from the heat and take out the lemon and cinnamon.\u003C\/li\u003E\n  \u003Cli\u003ESlowly add the egg mixture to the hot milk through a strainer to ensure there are no lumps.\u003C\/li\u003E\n  \u003Cli\u003EReturn it to the heat until it boils, stirring constantly.\u003C\/li\u003E\n  \u003Cli\u003EOnce the mixture has begun to thicken, remove from the heat and put it into individual bowls to cool.\u003C\/li\u003E\n  \u003Cli\u003EFor an authentic finish, sprinkle with sugar and brown with a blowtorch or under a preheated broiler to caramelize.\u003C\/li\u003E\n\u003C\/ol\u003E\n",
          "format":"basic_html",
          "processed":"\u003Col\u003E\n\u003Cli\u003EMix the egg yolks, sugar, and cornstarch with 1\/3 of the milk.\u003C\/li\u003E\n\u003Cli\u003EIn a saucepan, boil 2\/3 of the milk with the lemon peel and cinnamon.\u003C\/li\u003E\n\u003Cli\u003EAfter 3 minutes, remove from the heat and take out the lemon and cinnamon.\u003C\/li\u003E\n\u003Cli\u003ESlowly add the egg mixture to the hot milk through a strainer to ensure there are no lumps.\u003C\/li\u003E\n\u003Cli\u003EReturn it to the heat until it boils, stirring constantly.\u003C\/li\u003E\n\u003Cli\u003EOnce the mixture has begun to thicken, remove from the heat and put it into individual bowls to cool.\u003C\/li\u003E\n\u003Cli\u003EFor an authentic finish, sprinkle with sugar and brown with a blowtorch or under a preheated broiler to caramelize.\u003C\/li\u003E\n\u003C\/ol\u003E\n"
        },
        "field_summary":{
          "value":"Enjoy this sweet recipe for one of the oldest desserts in Europe. It requires very few ingredients!",
          "format":"basic_html",
          "processed":"\u003Cp\u003EEnjoy this sweet recipe for one of the oldest desserts in Europe. It requires very few ingredients!\u003C\/p\u003E\n"
        }
      },
