{
  "name": "druxt-site",
  "version": "0.6.0",
  "description": "Out of the box decoupled Drupal Site module for DruxtJS.",
  "keywords": [
    "drupal",
    "druxt",
    "jsonapi",
    "nuxt"
  ],
  "exports": {
    ".": {
      "require": "./dist/druxt-site.ssr.js",
      "import": "./dist/druxt-site.esm.js"
    },
    "./components/*": "./dist/components/*"
  },
  "main": "dist/druxt-site.ssr.js",
  "module": "dist/druxt-site.esm.js",
  "files": [
    "dist",
    "nuxt"
  ],
  "dependencies": {
    "@nuxtjs/proxy": "^2.1.0",
    "druxt": "^0.8.3",
    "druxt-blocks": "^0.10.0",
    "druxt-breadcrumb": "^0.9.0",
    "druxt-entity": "^0.16.0",
    "druxt-menu": "^0.11.0",
    "druxt-router": "^0.18.1",
    "druxt-schema": "^0.7.7",
    "druxt-views": "^0.11.0",
    "md5": "^2.2.1"
  },
  "peerDependencies": {
    "vuex": "^3.6.0"
  }
}
