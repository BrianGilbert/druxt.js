{
  "name": "druxt-blocks",
  "version": "0.10.0",
  "description": "Provides Drupal blocks and region components to be used within a Druxt (DRUpal nuXT) project.",
  "keywords": [
    "block",
    "blocks",
    "drupal",
    "druxt",
    "nuxt"
  ],
  "license": "MIT",
  "author": {
    "name": "Stuart Clark",
    "email": "stuart@realityloop.com",
    "url": "https://realityloop.com"
  },
  "exports": {
    ".": {
      "require": "./dist/druxt-blocks.ssr.js",
      "import": "./dist/druxt-blocks.esm.js"
    },
    "./components/*": "./dist/components/*",
    "./mixins/*": "./dist/mixins/*"
  },
  "main": "dist/druxt-blocks.ssr.js",
  "module": "dist/druxt-blocks.esm.js",
  "files": [
    "dist",
    "nuxt"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "drupal-jsonapi-params": "^1.1.12",
    "druxt": "^0.8.2",
    "druxt-entity": "^0.16.0",
    "druxt-router": "^0.18.1"
  },
  "peerDependencies": {
    "vuex": "^3.6.0"
  }
}
