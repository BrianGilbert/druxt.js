{
  "name": "druxt-breadcrumb",
  "version": "0.9.0",
  "description": "Provides a Breadcrumb Vue component to be used within a Druxt (DRUpal nuXT) project.",
  "keywords": [
    "breadcrumb",
    "breadcrumbs",
    "drupal",
    "druxt",
    "nuxt"
  ],
  "license": "MIT",
  "author": {
    "name": "Stuart Clark",
    "email": "stuart@realityloop.com",
    "url": "https://realityloop.com"
  },
  "exports": {
    ".": {
      "require": "./dist/druxt-breadcrumb.ssr.js",
      "import": "./dist/druxt-breadcrumb.esm.js"
    },
    "./components/*": "./dist/components/*",
    "./mixins/*": "./dist/mixins/*"
  },
  "main": "dist/druxt-breadcrumb.ssr.js",
  "module": "dist/druxt-breadcrumb.esm.js",
  "files": [
    "dist",
    "nuxt"
  ],
  "dependencies": {
    "druxt": "^0.8.2",
    "druxt-blocks": "^0.10.0",
    "druxt-router": "^0.18.1",
    "vuex": "^3.6.0"
  }
}
